[run]
branch = True
source = numba_dppy
omit = numba_dppy/tests/*
concurrency = multiprocessing
parallel = True

[report]
precision = 2
; show_missing = True
; sort = -cover
omit =
  # Vendored packages
  numba_dppy/_version.py
exclude_lines =
  pragma: no cover
  raise NotImplementedError
  if config.DEBUG:
  # numba compiled code
  @register_jitable
  def dpnp_impl
